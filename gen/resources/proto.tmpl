syntax = "proto3";
{{range .Imports}}
import "{{.}}";
{{end}}
package {{.Package}};
{{range .Options}}
option {{.Name}} = {{.Value}};
{{end}}
{{range .Services}}
{{if ne .Comment ""}}
//{{.Comment}}
{{end}}
service {{.Name}}{
   {{range .Rpcs}}
    {{if ne .Comment ""}}
    //{{.Comment}}
    {{end}}
    rpc {{.Name}}({{.Request}}) returns ({{.Response}}){
        {{range .Options}}
          option {{.Name}} = { 
           {{.Value}}
        {{end}}
    };
   {{end}}
};
{{end}}
{{range .Messages}}
{{if ne .Comment ""}}
//{{.Comment}}
{{end}}
message {{.Name}}{
    {{range .Fields}}
      {{if ne .Comment ""}}
      //{{.Comment}}
      {{end}}
       {{if ne .Options ""}}
         {{.Options}}
        {{else}}
          {{.Type}} {{.Name}} = {{.Number}} ;
        {{end}}
    {{end}}
   {{if .Oneof}}
     oneof {{.Oneof.Name}} {
      {{range .Oneof.Fields}}
        {{if ne .Comment ""}}
        //{{.Comment}}
        {{end}}
        {{if ne .Options ""}}
         {{.Options}}
        {{else}}
          {{.Type}} {{.Name}} = {{.Number}} ;
        {{end}}
      {{end}}
      }
    {{end}}
}
{{end}}