syntax = "proto3";
{{range .Imports}}
import "{{.}}";
{{end}}
package {{.Package}};
{{range .Options}}
option {{.Name}} = {{.Value}};
{{end}}
{{range .Services}}
{{range .Comment}}
//{{.}}
{{end}}
service {{.Name}}{
   {{range .Rpcs}}
    {{range .Comment}}
    //{{.}}
    {{end}} 
    rpc {{.Name}}({{.Request}}) returns ({{.Response}}){
        {{range .Options}}
          option {{.Name}} = { 
           {{.Value}}
        {{end}}
    };
   {{end}}
};
{{end}}
{{range .Messages}}
{{range .Comment}}
//{{.}}
{{end}}
message {{.Name}}{
    {{range .Fields}}
    {{range .Comment}}
    //{{.}}
    {{end}}
       {{if ne .Options ""}}
         {{.Options}}
        {{else}}
          {{.Type}} {{.Name}} = {{.Number}} ;
        {{end}}
    {{end}}
   {{if .Oneof}}
     oneof {{.Oneof.Name}} {
      {{range .Oneof.Fields}}
        {{range .Comment}}
        //{{.}}
        {{end}}
        {{if ne .Options ""}}
         {{.Options}}
        {{else}}
          {{.Type}} {{.Name}} = {{.Number}} ;
        {{end}}
      {{end}}
      }
    {{end}}
    {{range .Messages}}
      {{range .Comment}}
      //{{.}}
      {{end}}
      message {{.Name}}{
          {{range .Fields}}
          {{range .Comment}}
          //{{.}}
          {{end}}
            {{if ne .Options ""}}
              {{.Options}}
              {{else}}
                {{.Type}} {{.Name}} = {{.Number}} ;
              {{end}}
          {{end}}
        {{if .Oneof}}
          oneof {{.Oneof.Name}} {
            {{range .Oneof.Fields}}
              {{range .Comment}}
              //{{.}}
              {{end}}
              {{if ne .Options ""}}
              {{.Options}}
              {{else}}
                {{.Type}} {{.Name}} = {{.Number}} ;
              {{end}}
            {{end}}
            }
          {{end}}
      }
      {{end}}
}
{{end}}